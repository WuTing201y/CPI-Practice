#include <stdio.h>
#include <math.h>

int main() {
    int ox,oy,oz,px,py,pz,qx,qy,qz;
    while(scanf("%d %d %d %d %d %d %d %d %d" ,&ox,&oy,&oz,&px,&py,&pz,&qx,&qy,&qz) != EOF){
        double pox = px - ox;
        double poy = py - oy;
        double poz = pz - oz;
        double qox = qx - ox;
        double qoy = qy - oy;
        double qoz = qz - oz;
        
        double poa = sqrt(pox*pox + poy*poy + poz*poz);
        double qob = sqrt(qox*qox + qoy*qoy + qoz*qoz);
        
        double ab = pox*qox + poy*qoy + poz*qoz;
        
        double result = ab / (poa*qob);
        
        printf("The cosine theta between vectors (%d, %d, %d) and (%d, %d, %d) is %.2f\n",(int)pox,(int)poy,(int)poz,(int)qox,(int)qoy,(int)qoz,result);        
    }
    return 0;
}

/*my result
-459 -51 -172 -26 -350 209 -33 -171 436
The cosine theta between vectors (433, -299, 381) and (426, -120, 608) is 0.93

